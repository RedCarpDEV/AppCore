    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:theming="http://redcarpdesigns.com/controls"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <theming:SkinDictionary>
                    <theming:SkinDictionary.Skin>
                        <theming:Skin Name="Skin1" Source="/RCd.AppCore.Ui.Desktop.Controls;component/Resources/Brushes.xaml" />
                    </theming:SkinDictionary.Skin>
                </theming:SkinDictionary>
                <ResourceDictionary Source="/RCd.AppCore.Ui.Desktop.Controls;component/Themes/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
